@article{ ISI:000254281700004,
Author = {Lisle, D. A. and Johnstone, S. A.},
Title = {{Usefulness of muscle denervation as an MRI sign of peripheral nerve
   pathology}},
Journal = {{AUSTRALASIAN RADIOLOGY}},
Year = {{2007}},
Volume = {{51}},
Number = {{6}},
Pages = {{516-526}},
Month = {{DEC}},
Abstract = {{Peripheral nerve disorders may be classified into compressive or
   entrapment neuropathies and non-compressive neuropathies. Muscle
   denervation recognized on MRI may be a useful sign in the diagnosis of
   peripheral nerve disorders. Acute or subacute denervation results in
   prolonged T2 relaxation time, producing increased signal in skeletal
   muscle on short tau inversion-recovery and fat-suppressed T2-weighted
   images. Chronic denervation produces fatty atrophy of skeletal muscles,
   resulting in increased muscle signal on T1-weighted images. This review
   will outline and illustrate the various ways that muscle denervation as
   seen on MRI may assist in the diagnosis and localization of peripheral
   nerve disorders.}},
