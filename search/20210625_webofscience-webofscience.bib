@article{ ISI:000417762300007,
Author = {Zheng, Chu-Shan and Zhang, Xiang and Chen, Yue-Yao and Zhang, Fang and
   Duan, Xiao-Hui and Chen, Mei-Wei and Lu, Lie-Jing and Shen, Jun},
Title = {{ASSESSMENT OF THE SYNERGIC EFFECT OF IMMUNOMODULATION ON NERVE REPAIR
   USING MULTIPARAMETRIC MAGNETIC RESONANCE IMAGING}},
Journal = {{MUSCLE \& NERVE}},
Year = {{2018}},
Volume = {{57}},
Number = {{1}},
Pages = {{E38-E45}},
Month = {{JAN}},
Abstract = {{Introduction: The immune system plays a pivotal role in nerve injury.
   The aim of this study was to determine the role of multiparametric
   magnetic resonance imaging (MRI) in evaluation of the synergic effect of
   immunomodulation on nerve regeneration in neurotmesis. Methods: Rats
   with sciatic nerve neurotmesis and surgical repair underwent serial
   multiparametric MR examinations over an 8-week period after
   subepineurial microinjection of lipopolysaccharide (LPS) and subsequent
   subcutaneous injection of FK506 or subepineurial microinjection of LPS
   or phosphate-buffered saline (PBS) alone. Results: Nerves treated with
   immunomodulation showed more prominent regeneration than those treated
   with LPS or PBS alone and more rapid restoration toward normal T2,
   fractional anisotropy (FA), and radial diffusivity (RD) values than
   nerves injected with LPS or PBS. Discussion: Nerves treated with
   immunomodulation exert synergic beneficial effects on nerve regeneration
   that can be predicted by T2 measurements and FA and RD values.}},
